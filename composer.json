{
    "name": "voidinsight/taxtotum-libs",
    "description": "A little framework to support Taxtotum Project",
    "type": "library",
    "authors": [
        {
            "name": "Alberto Clemente",
            "email": "info@voidinsight.it"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "VoidInsight\\Taxtotum\\Libs\\": "src/",
            "VoidInsight\\Taxtotum\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --testdox --coverage-html docs/phpunit"
        ],
        "docs": [
            "bin/phpDocumentor.phar -d src/ -t docs/api",
            "git add docs/*",
            "git commit -m \"Docs updated\""
        ]
    }
}
